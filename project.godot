; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="2Caras1Moto"
config/description="Bem vindo a *2 CARAS 1 MOTO*! O jogo onde você deve escapar da policia 
e roubar dinheiro de quem passar pela sua frente, enquanto evita os
vários obstáculos no caminho."
run/main_scene="res://scenes/scene_controller.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://assets/icon.png"
config/windows_native_icon="res://assets/icon.ico"

[display]

window/stretch/mode="canvas_items"

[importer_defaults]

texture={
"detect_3d/compress_to": 0,
"mipmaps/generate": true
}

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/lossless_compression/force_png=true
